!Backpack methodsFor: 'accessing' stamp: 'MF 9/19/2021 20:10:40' prior: 50917067!
totalWeight
	
	^container sum: [ :portableObject | ]

	"| total index |

	total := 0.
	index := 1.
	
	[index <= container size] whileTrue: [ | portableObject |
		portableObject := (container at: index).
		total := total + portableObject weight.
		index := index + 1.	
	].

	^ total."! !
