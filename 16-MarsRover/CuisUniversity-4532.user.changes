!SmallInteger methodsFor: 'romans' stamp: 'MF 9/30/2021 22:10:53' prior: 50936786!
asRomanWithTens: tens fives: fives units: units
	
	| romanizedNumber |
	romanizedNumber _ ''.
	romanizedNumber _ self convertToRomanIfBetween: 0 and: 3 with: '' previousYYY: romanizedNumber withXX: units.
	
	self = 4 ifTrue: [ ^ (units, fives) ].	
	romanizedNumber _ self convertToRomanIfBetween: 5 and: 8 with: fives previousYYY: romanizedNumber withXX: units.

	self = 9 ifTrue: [^ (units, tens)].
	^romanizedNumber.! !

