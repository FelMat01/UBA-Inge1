!FibonacciCalculator class methodsFor: 'as yet unclassified' stamp: 'MF 9/16/2021 18:55:13' prior: 50896706!
for: unEntero

	| fibonacciDictionary blockClosure |
	
	fibonacciDictionary := Dictionary new
		add: [(unEntero isOne or:[unEntero isZero])]->[Entero with: 1];
		add: [unEntero isNegative]->[self error: Entero negativeFibonacciErrorDescription];
		add: [(unEntero isOne or: [unEntero isZero] or: [unEntero isNegative]) not]->[(unEntero - (Entero with: 1)) fibonacci + (unEntero - (Entero with: 2)) fibonacci].
	
	blockClosure := fibonacciDictionary keys detect: [ :condition | condition value: unEntero ].
		
	^fibonacciDictionary at: blockClosure ifAbsent: [self error: 'No way found to calculate fibonacci'.].
	
	
	"(unEntero isOne or:[unEntero isZero]) ifTrue:[^[Entero with: 1]].
	unEntero isNegative ifTrue:[^[self error: Entero negativeFibonacciErrorDescription]].
	(unEntero isOne or: [unEntero isZero] or: [unEntero isNegative]) not
		ifTrue:[^(unEntero - (Entero with: 1)) fibonacci + (unEntero - (Entero with: 2)) fibonacci]."
		
	! !

