!Room methodsFor: 'contents' stamp: 'MF 9/22/2021 23:20:52' prior: 50925162!
moveFrom: aSourcePosition to: aTargetPosition

	| toMove target |
	
	toMove := self at: aSourcePosition.
	target := self at: aTargetPosition ifAbsent: [ 
		self at: aTargetPosition put: toMove.
		^self removeAt: aSourcePosition ].
	
	(target isKindOf: Hole) 
		ifTrue: [ 
			self at: target targetPosition put: toMove.
			^contents removeKey: aSourcePosition ]
		ifFalse: [
			self at: aTargetPosition put: toMove.
			^contents removeKey: aSourcePosition ].
			! !

