assertCanNotJumpObject: aNonJumpableObject

	| room playableCharacter |
	
	room := self defaultRoom.
	room at: 0@2 put: aNonJumpableObject .
	playableCharacter := PlayableCharacter named: 'Luigi' using: self defaultBackpack at: 0@3 heading: #South in: room.
	
	self 
		should: [ playableCharacter jump ]
		raise: Error - MessageNotUnderstood 
		withExceptionDo: [:anError |
			self assert: PlayableCharacter canNotJumpErrorDescription equals: anError messageText.
			self assert: (playableCharacter isAt: 0@3 heading: #South) ]
	! !
