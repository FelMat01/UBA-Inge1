!Room methodsFor: 'contents' stamp: 'MF 9/22/2021 23:56:16' prior: 50927822!
moveFrom: aSourcePosition to: aTargetPosition

	| target |
	
	target := self at: aTargetPosition ifAbsent: [ 
		self at: aTargetPosition put: (self at: aSourcePosition).
		^self removeAt: aSourcePosition ].
	
	target moveInRoom: self objectAt: aSourcePosition toPositionIfNotHole: aTargetPosition.
